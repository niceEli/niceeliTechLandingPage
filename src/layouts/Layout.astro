---
import "../global.css"
const {title, icon} = Astro.props;
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>{title}</title>
    <link rel="icon" type="image/svg+xml" href={icon} />
  </head>
  <body>
    <slot />
  </body>
</html>